QComboBox {
    border: 1px solid #CCCCCC;
    border-radius: 3px;
    height: 24px;
}

QComboBox::drop-down {
    subcontrol-origin: border;
    subcontrol-position: center right;
    width:26px;
    border-left-width: 0px;
    border-left-color: #CCCCCC;
    border-left-style:solid;
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
    width:26px;
    height:26px;
    image: url(://pictures/btn_down.png);
}

HNMainForm HNTabWidget#tabWidget HNTabBar#tabWidget_bar::tab {
    padding: 0px;
    margin: 0px;
    width: 76 ex;
    height: 72 px;
}

QTabWidget#tabWidget_set::pane, QTabWidget#tabWidget_help::pane,
QTabWidget#tabWidget_user::pane, QTabWidget#tabWidget_dispel::pane,
QTabWidget#tabWidget_edit::pane,QTabWidget#tabWidget_cloud::pane { /* The tab widget frame */
    border: 1px solid #FF8400;
    background-color: #FFFFFF;
    top:-1px;
}

QTabWidget#tabWidget_set QTabBar::tab, QTabWidget#tabWidget_help QTabBar::tab,
QTabWidget#tabWidget_user QTabBar::tab, QTabWidget#tabWidget_edit QTabBar::tab,
QTabWidget#tabWidget_dispel QTabBar::tab,QTabWidget#tabWidget_cloud QTabBar::tab{
    width: 90px;
    height: 30px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border: 1px solid #FF8400;
    border-bottom: none;
    padding: 0px;
    margin-right: 5px;
    background-color: #FF8400;
    color: #FFFFFF;
}

QTabWidget#tabWidget_set QTabBar::tab:selected, QTabWidget#tabWidget_help QTabBar::tab:selected,
QTabWidget#tabWidget_user QTabBar::tab:selected, QTabWidget#tabWidget_dispel QTabBar::tab:selected,
QTabWidget#tabWidget_edit QTabBar::tab:selected, QTabWidget#tabWidget_cloud QTabBar::tab:selected{
    background-color: #FFFFFF;
    color: #FF8400;
}

QLineEdit {
    min-height: 24px;
    background-color: rgba(255,255,255,255);
    border: 1px solid #CCCCCC;
    border-radius: 3px;
}

QLineEdit:focus {
    background:white;
    border-image:none;
    border:1px solid #FF8400;
    border-radius:3px;
}

QSpinBox, QDateTimeEdit, QDateEdit, QTimeEdit, QDoubleSpinBox{
    min-height: 24px;
    background-color: rgba(255,255,255,255);
    border: 1px solid #CCCCCC;
    border-radius: 3px;
}

QSpinBox:focus, QDateTimeEdit:focus, QDateEdit:focus, QTimeEdit:focus, QDoubleSpinBox:focus{
    background-color: rgba(255,255,255,255);
    border: 1px solid #FF8400;
    border-radius: 3px;
}

QDateTimeEdit::up-button, QDateEdit::up-button, QTimeEdit::up-button,
QDoubleSpinBox::up-button,QSpinBox::up-button {
    subcontrol-origin:border;
    subcontrol-position:right;
    border-image: url(://pictures/btn_up.png);
    width: 28px;
    height: 28px;
}

QDateTimeEdit::up-button:focus, QDateEdit::up-button:focus, QTimeEdit::up-button:focus,
QDoubleSpinBox::up-button:focus,QSpinBox::up-button:focus {
    subcontrol-origin:border;
    subcontrol-position:right;
    border-image: url(://pictures/btn_up_focus.png);
    width: 28px;
    height: 28px;
}

QDateTimeEdit::up-button:pressed, QDateEdit::up-button:pressed, QTimeEdit::up-button:pressed,
QDoubleSpinBox::up-button:pressed,QSpinBox::up-button:pressed{
    subcontrol-origin:border;
    subcontrol-position:right;
    border-image: url(://pictures/btn_up_press.png);
    width: 28px;
    height: 28px;
}

QDateTimeEdit::down-button,QDateEdit::down-button,QTimeEdit::down-button,
QDoubleSpinBox::down-button,QSpinBox::down-button {
    subcontrol-origin:border;
    subcontrol-position:left;
    border-image: url(://pictures/btn_down.png);
    width: 28px;
    height: 28px;
}

QDateTimeEdit::down-button:focus,QDateEdit::down-button:focus,QTimeEdit::down-button:focus,
QDoubleSpinBox::down-button:focus,QSpinBox::down-button:focus {
    subcontrol-origin:border;
    subcontrol-position:left;
    border-image: url(://pictures/btn_down_focus.png);
    width: 28px;
    height: 28px;
}

QDateTimeEdit::down-button:pressed,QDateEdit::down-button:pressed,QTimeEdit::down-button:pressed,
QDoubleSpinBox::down-button:pressed,QSpinBox::down-button:pressed{
    subcontrol-origin:border;
    subcontrol-position:left;
    border-image: url(://pictures/btn_down_press.png);
    width: 28px;
    height: 28px;
}

